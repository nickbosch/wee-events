{
  "name": "@weegigs/effects",
  "version": "0.16.0",
  "description": "Effects utilities for Wee Events",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@3.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/weegigs/wee-events.git"
  },
  "scripts": {
    "build": "yarn ws:build",
    "clean": "yarn ws:clean",
    "compile": "yarn ws:compile",
    "test": "yarn ws:test",
    "lint": "yarn ws:lint",
    "doctor": "yarn ws:doctor",
    "fix": "yarn run ws:fix",
    "publish": "yarn run ws:publish"
  },
  "author": "Kevin O'Neill <kevin@weegigs.com.au>",
  "license": "ISC",
  "dependencies": {
    "@effect-ts/node": "^0.38.3",
    "@middy/core": "^2.5.7",
    "@middy/do-not-wait-for-empty-event-loop": "^2.5.7",
    "aws-lambda": "^1.0.7",
    "senselogs": "^1.0.2"
  },
  "peerDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.54.0",
    "@aws-sdk/client-dynamodb": "^3.54.0",
    "@aws-sdk/client-eventbridge": "^3.54.0",
    "@aws-sdk/client-sfn": "^3.54.0",
    "@effect-ts/core": "^0.58.0",
    "@weegigs/dynamo-event-store": "workspace:^",
    "@weegigs/events-core": "workspace:^"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.54.0",
    "@aws-sdk/client-dynamodb": "^3.54.0",
    "@aws-sdk/client-eventbridge": "^3.54.0",
    "@aws-sdk/client-sfn": "^3.54.0",
    "@effect-ts/core": "^0.58.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.12",
    "@weegigs/dynamo-event-store": "workspace:^",
    "@weegigs/events-core": "workspace:^",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "html"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
