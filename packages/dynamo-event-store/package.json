{
  "name": "@weegigs/dynamo-event-store",
  "version": "0.15.0",
  "description": "DynamoDB event storage for Wee Events",
  "author": "Kevin O'Neill <kevin@weegigs.com.au>",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "yarn ws:build",
    "clean": "yarn ws:clean",
    "compile": "yarn ws:compile",
    "test": "yarn ws:test",
    "lint": "yarn ws:lint",
    "doctor": "yarn ws:doctor",
    "fix": "yarn run ws:fix"
  },
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.49.0",
    "@aws-sdk/service-error-classification": "^3.49.0",
    "@aws-sdk/smithy-client": "^3.49.0",
    "@effect-ts/node": "^0.36.0",
    "@weegigs/effects": "workspace:*",
    "@weegigs/events-common": "workspace:*",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "ulid": "^2.3.0",
    "zod": "^3.11.6"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "@effect-ts/core": "^0.55.1",
    "@weegigs/events-cypher": "workspace:^",
    "@weegigs/wee-events": "workspace:^"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "@aws-sdk/types": "^3.49.0",
    "@effect-ts/core": "^0.55.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/node": "^14.18.10",
    "@weegigs/events-cypher": "workspace:*",
    "@weegigs/wee-events": "workspace:*",
    "jest": "^27.5.0",
    "testcontainers": "^8.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "html"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
