{
  "name": "@weegigs/dynamo-event-store",
  "version": "0.16.0",
  "description": "DynamoDB event storage for Wee Events",
  "author": "Kevin O'Neill <kevin@weegigs.com.au>",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "packageManager": "yarn@3.2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/weegigs/wee-events.git"
  },
  "scripts": {
    "build": "yarn ws:build",
    "clean": "yarn ws:clean",
    "compile": "yarn ws:compile",
    "test": "yarn ws:test",
    "lint": "yarn ws:lint",
    "doctor": "yarn ws:doctor",
    "fix": "yarn run ws:fix",
    "publish": "yarn run ws:publish"
  },
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.100.0",
    "@aws-sdk/service-error-classification": "^3.78.0",
    "@aws-sdk/smithy-client": "^3.99.0",
    "@weegigs/events-common": "workspace:*",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "ulid": "^2.3.0",
    "zod": "^3.17.3"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.54.0",
    "@weegigs/events-core": "workspace:^",
    "@weegigs/events-cypher": "workspace:^"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.100.0",
    "@aws-sdk/types": "^3.78.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/node": "^14.18.18",
    "@weegigs/events-core": "workspace:*",
    "@weegigs/events-cypher": "workspace:*",
    "jest": "^28.1.0",
    "testcontainers": "^8.10.1",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "html"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
